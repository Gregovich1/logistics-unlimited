                <li data-target="#carouselExampleIndicators" data-slide-to="4"></li>
            </ol>
    <div class="carousel-item active">
      <img class="d-block w-100" src="img/home01.jpg" alt="First slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="img/home02.jpg" alt="Second slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="img/home07.jpg" alt="Third slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="img/home08.jpg" alt="Second slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="img/home06.jpg" alt="Second slide">
    </div>
  </div>

            {{> footer }}
        <script src="/src/app.js"></script>
